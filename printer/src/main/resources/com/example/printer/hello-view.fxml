<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.printer.HelloController">
   <children>
      <Button fx:id="print" layoutX="257.0" layoutY="200.0" onAction="#onHelloButtonClick" prefHeight="38.0" prefWidth="86.0" style="-fx-background-color: #0598ff;" text="Print" textFill="WHITE">
         <font>
            <Font size="21.0" />
         </font></Button>
      <Button fx:id="closeButton" layoutX="562.0" layoutY="14.0" mnemonicParsing="false" onAction="#onCloseButtonClick" text="X" />
      <Label fx:id="welcomeText" layoutX="44.0" layoutY="36.0" prefHeight="150.0" prefWidth="502.0" />
      <ComboBox fx:id="comboBox" layoutX="44.0" layoutY="111.0" onAction="#onComboBoxSelect" prefHeight="26.0" prefWidth="214.0" promptText="Choose Printer" style="-fx-background-color: transparent; -fx-border-color: #0598ff; -fx-border-width: 0px 0px 2px 0px;" />
      <Label layoutX="257.0" layoutY="46.0" prefHeight="38.0" prefWidth="101.0" text="Printer">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
